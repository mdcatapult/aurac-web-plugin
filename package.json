{
  "name": "browser-plugin",
  "version": "0.11.0",
  "scripts": {
    "ng": "ng",
    "start": "web-ext run --verbose --source-dir=dist/browser-plugin",
    "build": "ng build --configuration production --source-map false && webpack",
    "dev": "ng build --source-map true && webpack && web-ext run --verbose --source-dir=dist/browser-plugin",
    "package": "cd dist/browser-plugin && web-ext build",
    "test": "jasmine && ng test",
    "test:ci": "npm i --save-dev @types/jest && jest --coverage && ng test --browsers GitlabCI --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production",
    "weblint": "cd dist/browser-plugin && web-ext lint",
    "version": "VERSION=$(jq -r '.version' package.json) && jq --arg v $VERSION '.version = $v' src/manifest.json > tmp.json && mv tmp.json src/manifest.json && git add src/manifest.json"
  },
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.2",
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@material/typography": "^11.0.0",
    "file-saver": "^2.0.5",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/chrome": "0.0.125",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "jasmine": "^3.9.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.4.0",
    "jest": "^27.2.1",
    "jest-puppeteer": "^5.0.4",
    "jest-without-globals": "0.0.3",
    "karma": "6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^11.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^10.4.0",
    "puppeteer-core": "^10.4.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.5",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "web-ext": "^5.4.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.7.0",
    "webpack-cli": "^4.8.0"
  }
}
