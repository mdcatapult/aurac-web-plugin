{
  "name": "browser-plugin",
  "version": "0.12.15",
  "scripts": {
    "ng": "ng",
    "start": "web-ext run --verbose --source-dir=dist/browser-plugin",
    "build": "ng build --configuration production --source-map false && webpack",
    "dev": "ng build --source-map true && webpack && web-ext run --verbose --source-dir=dist/browser-plugin",
    "package": "cd dist/browser-plugin && web-ext build",
    "test": "jasmine-ts && ng test",
    "test:ci": "jasmine-ts && ng test --browsers GitlabCI --watch false",
    "mocha": "mocha mocha-test.js",
    "lint": "npx ng lint browser-plugin && npx prettier --check .",
    "lint:fix": "npx ng lint browser-plugin --fix && npx prettier --write .",
    "e2e": "ng e2e",
    "wp": "webpack",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production",
    "build:bio": "cross-env NODE_ENV=production ng build --configuration bio",
    "unopt": "ng build --configuration production --optimization=false && webpack",
    "weblint": "cd dist/browser-plugin && web-ext lint",
    "version": "VERSION=$(jq -r '.version' package.json) && jq --arg v $VERSION '.version = $v' src/manifest.json > tmp.json && mv tmp.json src/manifest.json && git add src/manifest.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.2",
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@material/typography": "^11.0.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "ngx-page-scroll": "^7.0.6",
    "ngx-page-scroll-core": "^7.0.6",
    "puppeteer": "^1.14.0",
    "puppeteer-core": "^1.14.0",
    "rxjs": "~6.5.4",
    "tippy.js": "^6.3.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/chrome": "0.0.125",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "^3.9.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.175",
    "@types/lodash.merge": "^4.6.6",
    "@types/mark.js": "^8.11.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "eslint": "^7.26.0",
    "file-loader": "^6.1.1",
    "jasmine": "^3.9.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.4.0",
    "jest-puppeteer": "^5.0.4",
    "jsdom": "17.0.0",
    "jsdom-global": "3.0.2",
    "karma": "6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mark.js": "^8.11.1",
    "mocha": "^6.1.3",
    "ngx-build-plus": "^11.0.0",
    "prettier": "2.4.1",
    "protractor": "~7.0.0",
    "puppeteer": "^9.0.0",
    "puppeteer-core": "^9.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.5",
    "ts-node": "^8.10.2",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "web-ext": "^5.4.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.7.0",
    "webpack-cli": "^4.8.0",
    "xvfb": "^0.4.0"
  }
}
